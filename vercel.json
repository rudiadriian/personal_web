{
    "version": 2,
    "outputDirectory": "public",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(css|js|images|rudi.png|favicon.svg)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
